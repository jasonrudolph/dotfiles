"*":
  Zen:
    softWrap: true
    typewriter: true
    width: 120
  "activate-power-mode":
    autoToggle: false
  "autocomplete-emojis":
    enableUnicodeEmojis: false
  "autocomplete-plus":
    enableAutoActivation: false
    minimumWordLength: 1
  core:
    customFileTypes:
      "source.ruby": [
        "god"
        "pp"
      ]
    destroyEmptyPanes: false
    disabledPackages: [
      "nvatom"
      "branch-status"
      "linter-rubocop"
      "indent-guide-improved"
      "linter"
      "linter-jshint"
      "linter-ui-default"
      "busy-signal"
      "intentions"
      "nuclide"
    ]
    ignoredNames: [
      ".DS_Store"
      ".git"
      "vendor"
    ]
    packagesWithKeymapsDisabled: [
      "platformio-ide-terminal"
    ]
    telemetryConsent: "no"
    themes: [
      "nucleus-dark-ui"
      "dracula-syntax"
    ]
    titleBar: "custom"
  editor:
    autoIndentOnPaste: false
    fontSize: 11
    invisibles: {}
    scrollPastEnd: true
  "file-icons":
    onChanges: true
    revealTreeView: false
  "find-and-replace":
    projectSearchResultsPaneSplitDirection: "right"
  "git-projects":
    maxDepth: 1
    rootPath: "~/github;~/src"
  "highlight-line":
    enableBackgroundColor: false
  "markdown-writer": {}
  "merge-conflicts":
    gitPath: "bin/git"
  "notational-velocity":
    directory: "/Users/j/.notes"
  nvatom:
    directory: "/Users/j/.notes"
  "one-dark-ui": {}
  "one-light-ui": {}
  "outdoor-theme":
    lightLevelToSwitch: 5904360
  "project-folder":
    closeItemsForRemovedProject: true
    projectRootDirectories: [
      "~/github"
      "~/src"
      "~/.dotfiles"
    ]
  "spell-check":
    grammars: [
      "text.plain"
      "source.gfm"
      "text.git-commit"
    ]
  "split-diff":
    diffWords: true
    ignoreWhitespace: true
    leftEditorColor: "red"
    rightEditorColor: "green"
    syncHorizontalScroll: true
  "status-bar": {}
  tabs:
    enableVcsColoring: true
  welcome:
    showOnStartup: false
".git-commit.text":
  editor:
    preferredLineLength: 72
