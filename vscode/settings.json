{
    "window.zoomLevel": 1,
    "editor.fontSize": 11,
    "telemetry.enableTelemetry": false,
    "atomKeymap.promptV3Features": true,
    "editor.multiCursorModifier": "ctrlCmd",
    "openInGitHub.providerType": "github",
    "openInGitHub.useCommitSHAInURL": true,
    "eslint.autoFixOnSave": true,
    "git.enableCommitSigning": true,
    "editor.tabSize": 2,
    "workbench.colorCustomizations": {
        "statusBar.background": "#303030",
        "statusBar.noFolderBackground": "#222225",
        "statusBar.debuggingBackground": "#511f1f"
    },
    "editor.tabCompletion": "on",
    "editor.autoIndent": false,
    "workbench.colorTheme": "Default Dark+",
    "dayNightThemeSwitcher.dayTheme": "Default Light+",
    "dayNightThemeSwitcher.nightTheme": "Default Dark+",
    "dayNightThemeSwitcher.dayThemeCustomizations": {
        "statusBar.background": "#303030",
        "statusBar.noFolderBackground": "#222225",
        "statusBar.debuggingBackground": "#511f1f"
    },
    "dayNightThemeSwitcher.nightThemeCustomizations": {
        "statusBar.background": "#303030",
        "statusBar.noFolderBackground": "#222225",
        "statusBar.debuggingBackground": "#511f1f"
    },
    "editor.minimap.enabled": false,
}