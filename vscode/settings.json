{
  "atomKeymap.promptV3Features": true,
  "cSpell.diagnosticLevel": "Warning",
  "cSpell.enabledLanguageIds": ["markdown", "plaintext"],
  "diffEditor.ignoreTrimWhitespace": false,
  "diffEditor.renderSideBySide": false,
  "editor.copyWithSyntaxHighlighting": false,
  "editor.fontSize": 11,
  "editor.formatOnSave": false,
  "editor.minimap.enabled": false,
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.quickSuggestions": {
    "other": false,
    "comments": false,
    "strings": false
  },
  "editor.renderControlCharacters": false,
  "editor.renderWhitespace": "none",
  "editor.rulers": [100],
  "editor.tabCompletion": "on",
  "editor.tabSize": 2,
  "editor.unicodeHighlight.invisibleCharacters": false,
  "emmet.includeLanguages": {
    "erb": "html",
    "html.erb": "html",
    "njk": "html"
  },
  "emmet.showSuggestionsAsSnippets": true,
  "emmet.triggerExpansionOnTab": true,
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "explorer.openEditors.visible": 0,
  "extensions.ignoreRecommendations": false,
  "files.associations": {
    "*.pgsql": "postgres",
    "COMMIT_EDITMSG": "git-commit"
  },
  "cSpell.enabled": true,
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "git.enableCommitSigning": true,
  "gitlens.advanced.telemetry.enabled": false,
  "gitlens.codeLens.enabled": false,
  "gitlens.currentLine.enabled": false,
  "gitlens.views.searchAndCompare.pullRequests.enabled": false,
  "go.useLanguageServer": true,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "markdown.extension.toc.githubCompatibility": true,
  "problems.showCurrentInStatus": true,
  "redhat.telemetry.enabled": false,
  "ruby.format": "rubocop",
  "ruby.intellisense": "rubyLocate",
  "ruby.rubocop.useBundler": true,
  "ruby.useBundler": true,
  "ruby.useLanguageServer": true,
  "search.enableSearchEditorPreview": true,
  "security.workspace.trust.startupPrompt": "never",
  "shebang.associations": [
    {
      "pattern": "^#!/bin/bash$",
      "language": "shellscript"
    },
    {
      "pattern": "^#!/usr/bin/env bin/rails runner$",
      "language": "ruby"
    }
  ],
  "telemetry.enableTelemetry": false,
  "terminal.integrated.defaultLocation": "editor",
  "terminal.integrated.rendererType": "experimentalWebgl",
  "terminal.integrated.scrollback": 100000,
  "window.zoomLevel": 1,
  "workbench.colorCustomizations": {
    "statusBar.background": "#303030",
    "statusBar.noFolderBackground": "#222225",
    "statusBar.debuggingBackground": "#511f1f"
  },
  "workbench.colorTheme": "Monokai Pro",
  "workbench.editor.decorations.colors": false,
  "workbench.editor.historyBasedLanguageDetection": true,
  "workbench.editor.untitled.hint": "hidden",
  "workbench.editorAssociations": {
    "git-rebase-todo": "default"
  },
  "workbench.iconTheme": "Monokai Pro Icons",
  "workbench.list.automaticKeyboardNavigation": false,
  "workbench.preferredDarkColorTheme": "Monokai Pro",
  "workbench.preferredLightColorTheme": "Quiet Light",
  "workbench.preferredHighContrastColorTheme": "Light High Contrast Theme",
  "workbench.preferredHighContrastLightColorTheme": "Default High Contrast",
  "workbench.startupEditor": "newUntitledFile",
  "workbench.statusBar.feedback.visible": false,
  "editor.unicodeHighlight.allowedCharacters": {
    "Ô∏è": true
  },
  "[git-commit]": {
    "workbench.editor.languageDetection": false
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features",
    "editor.formatOnSave": true
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "[markdown]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.quickSuggestions": {
      "comments": "off",
      "strings": "off",
      "other": "off"
    },
    "editor.wordWrap": "on"
  },
  "[ruby]": {
    "editor.defaultFormatter": "misogi.ruby-rubocop"
  },
  "[yaml]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  }
}
